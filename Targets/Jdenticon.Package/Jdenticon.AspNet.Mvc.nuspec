<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Jdenticon.AspNet.Mvc</id>
    <version>$version$</version>
    <title>Jdenticon ASP.NET MVC Extensions</title>
    <authors>Daniel Mester Pirttijärvi</authors>
    <license type="expression">MIT</license>
    <projectUrl>http://jdenticon.com/</projectUrl>
    <icon>images\jdenticon-logo-net.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extends Jdenticon-net with features to use Jdenticon with ASP.NET MVC.

This package contains:
* @Html.Identicon() extension method
* @Url.Identicon() extension method
* IdenticonResult for usage in MVC controllers</description>
    <tags>identicon avatar library</tags>
    <dependencies>
      <dependency id="Jdenticon-net" version="$version$" />
      <dependency id="Microsoft.AspNet.Mvc" version="[4.0,)" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\releases\Jdenticon.$version$\net40\Jdenticon.AspNet.Mvc.dll" target="lib\net40\Jdenticon.AspNet.Mvc.dll" />
    <file src="..\..\releases\Jdenticon.$version$\net45\Jdenticon.AspNet.Mvc.dll" target="lib\net45\Jdenticon.AspNet.Mvc.dll" />
    <file src="..\..\releases\help\Jdenticon.AspNet.Mvc.xml" target="lib\net40\Jdenticon.AspNet.Mvc.xml" />
    <file src="..\..\releases\help\Jdenticon.AspNet.Mvc.xml" target="lib\net45\Jdenticon.AspNet.Mvc.xml" />
    <file src="Web.config.Mvc.transform" target="Content\Web.config.transform" />
    <file src="ReadMe.Mvc.txt" target="readme.txt" />
    <file src="nuget-logo-net.png" target="images\jdenticon-logo-net.png" />
  </files>
</package>