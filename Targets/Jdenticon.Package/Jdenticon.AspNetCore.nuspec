<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Jdenticon.AspNetCore</id>
    <version>$version$</version>
    <title>Jdenticon ASP.NET Core Extensions</title>
    <authors>Daniel Mester Pirttijärvi</authors>
    <license type="expression">MIT</license>
    <projectUrl>http://jdenticon.com/</projectUrl>
    <repository type="git" url="https://github.com/dmester/jdenticon-net.git" />
    <icon>images\jdenticon-logo-net.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extends Jdenticon-net with features to use Jdenticon with ASP.NET Core.

This package contains:
* IdenticonTagHelper
* @Html.Identicon() extension method
* @Url.Identicon() extension method
* IdenticonResult for usage in MVC controllers</description>
    <tags>identicon avatar library ASP.NET Core</tags>
    <dependencies>
      <group targetFramework="netstandard1.6">
        <dependency id="Jdenticon-net" version="$version$" />
        <dependency id="Microsoft.AspNetCore.Mvc.ViewFeatures" version="1.0.0" />
        <dependency id="Microsoft.AspNetCore.Razor.Runtime" version="1.0.0" />
      </group>
      <group targetFramework="netstandard2.1">
      </group>
      <group targetFramework="net451">
        <dependency id="Jdenticon-net" version="$version$" />
        <dependency id="Microsoft.AspNetCore.Mvc.ViewFeatures" version="1.0.0" />
        <dependency id="Microsoft.AspNetCore.Razor.Runtime" version="1.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\releases\Jdenticon.$version$\netstandard16\Jdenticon.AspNetCore.dll" target="lib\netstandard1.6\Jdenticon.AspNetCore.dll" />
    <file src="..\..\releases\Jdenticon.$version$\netstandard16\Jdenticon.AspNetCore.pdb" target="lib\netstandard1.6\Jdenticon.AspNetCore.pdb" />
    <file src="..\..\releases\Jdenticon.$version$\netstandard21\Jdenticon.AspNetCore.dll" target="lib\netstandard2.1\Jdenticon.AspNetCore.dll" />
    <file src="..\..\releases\Jdenticon.$version$\netstandard21\Jdenticon.AspNetCore.pdb" target="lib\netstandard2.1\Jdenticon.AspNetCore.pdb" />
    <file src="..\..\releases\Jdenticon.$version$\net451\Jdenticon.AspNetCore.dll" target="lib\net451\Jdenticon.AspNetCore.dll" />
    <file src="..\..\releases\Jdenticon.$version$\net451\Jdenticon.AspNetCore.pdb" target="lib\net451\Jdenticon.AspNetCore.pdb" />
    <file src="..\..\releases\help\Jdenticon.AspNetCore.xml" target="lib\netstandard1.6\Jdenticon.AspNetCore.xml" />
    <file src="..\..\releases\help\Jdenticon.AspNetCore.xml" target="lib\netstandard2.1\Jdenticon.AspNetCore.xml" />
    <file src="..\..\releases\help\Jdenticon.AspNetCore.xml" target="lib\net451\Jdenticon.AspNetCore.xml" />
    <file src="ReadMe.AspNetCore.txt" target="readme.txt" />
    <file src="nuget-logo-net.png" target="images\jdenticon-logo-net.png" />
  </files>
</package>