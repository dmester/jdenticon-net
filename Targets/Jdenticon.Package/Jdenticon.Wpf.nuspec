<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Jdenticon.Wpf</id>
    <version>$version$</version>
    <title>Jdenticon WPF Extensions</title>
    <authors>Daniel Mester Pirttijärvi</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://jdenticon.com/</projectUrl>
    <repository type="git" url="https://github.com/dmester/jdenticon-net.git" commit="$SourceRevisionId$" />
    <icon>images\jdenticon-logo-net.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extends Jdenticon-net with features to use Jdenticon with WPF.</description>
    <tags>identicon avatar library WPF</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Jdenticon-net" version="$version$" />
      </group>
      <group targetFramework="net45">
        <dependency id="Jdenticon-net" version="$version$" />
      </group>
      <group targetFramework="netcoreapp3.0">
        <dependency id="Jdenticon-net" version="$version$" />
      </group>
    </dependencies>
    <frameworkReferences>
      <group targetFramework="netcoreapp3.0">
        <frameworkReference name="Microsoft.WindowsDesktop.App.WPF" />
      </group>
    </frameworkReferences>
  </metadata>
  <files>
    <file src="..\..\releases\Jdenticon.$version$\net40\Jdenticon.Wpf.dll" target="lib\net40\Jdenticon.Wpf.dll" />
    <file src="..\..\releases\Jdenticon.$version$\net40\Jdenticon.Wpf.pdb" target="lib\net40\Jdenticon.Wpf.pdb" />
    <file src="..\..\releases\Jdenticon.$version$\net45\Jdenticon.Wpf.dll" target="lib\net45\Jdenticon.Wpf.dll" />
    <file src="..\..\releases\Jdenticon.$version$\net45\Jdenticon.Wpf.pdb" target="lib\net45\Jdenticon.Wpf.pdb" />
    <file src="..\..\releases\Jdenticon.$version$\netcore30\Jdenticon.Wpf.dll" target="lib\netcoreapp3.0\Jdenticon.Wpf.dll" />
    <file src="..\..\releases\Jdenticon.$version$\netcore30\Jdenticon.Wpf.pdb" target="lib\netcoreapp3.0\Jdenticon.Wpf.pdb" />
    <file src="..\..\releases\help\Jdenticon.Wpf.xml" target="lib\net40\Jdenticon.Wpf.xml" />
    <file src="..\..\releases\help\Jdenticon.Wpf.xml" target="lib\net45\Jdenticon.Wpf.xml" />
    <file src="..\..\releases\help\Jdenticon.Wpf.xml" target="lib\netcoreapp3.0\Jdenticon.Wpf.xml" />
    <file src="ReadMe.Wpf.txt" target="readme.txt" />
    <file src="nuget-logo-net.png" target="images\jdenticon-logo-net.png" />
  </files>
</package>