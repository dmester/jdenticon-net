<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Jdenticon.AspNet.WebApi</id>
    <version>$version$</version>
    <title>Jdenticon ASP.NET WebApi Extensions</title>
    <authors>Daniel Mester Pirttijärvi</authors>
    <license type="expression">MIT</license>
    <projectUrl>http://jdenticon.com/</projectUrl>
    <icon>images\jdenticon-logo-net.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extends Jdenticon-net with features to use Jdenticon with ASP.NET WebApi.

This package contains:
* IdenticonResult for usage in WebApi controllers</description>
    <tags>identicon avatar library ASP.NET WebApi</tags>
    <dependencies>
      <dependency id="Jdenticon-net" version="$version$" />
      <dependency id="Microsoft.AspNet.WebApi.Core" version="4.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\releases\Jdenticon.$version$\net45\Jdenticon.AspNet.WebApi.dll" target="lib\net45\Jdenticon.AspNet.WebApi.dll" />
    <file src="..\..\releases\help\Jdenticon.AspNet.WebApi.xml" target="lib\net45\Jdenticon.AspNet.WebApi.xml" />
    <file src="ReadMe.WebApi.txt" target="readme.txt" />
    <file src="nuget-logo-net.png" target="images\jdenticon-logo-net.png" />
  </files>
</package>