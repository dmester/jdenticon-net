<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Jdenticon.WinForms</id>
    <version>$version$</version>
    <title>Jdenticon WinForms Extensions</title>
    <authors>Daniel Mester Pirttijärvi</authors>
    <license type="expression">MIT</license>
    <projectUrl>http://jdenticon.com/</projectUrl>
    <icon>images\jdenticon-logo-net.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extends Jdenticon-net for usage in Windows Forms applications.

Features:
* IdenticonView control</description>
    <tags>identicon avatar library WinForms</tags>
    <dependencies>
      <group targetFramework="netcoreapp3.0">
        <dependency id="Jdenticon-net" version="$version$" />
        <dependency id="Jdenticon.Gdi" version="$version$" />
        <dependency id="System.Drawing.Common" version="4.5.0" />
      </group>
      <group targetFramework="net40">
        <dependency id="Jdenticon-net" version="$version$" />
        <dependency id="Jdenticon.Gdi" version="$version$" />
      </group>
    </dependencies>
    <frameworkReferences>
      <group targetFramework="netcoreapp3.0">
        <frameworkReference name="Microsoft.WindowsDesktop.App.WindowsForms" />
      </group>
    </frameworkReferences>
  </metadata>
  <files>
    <file src="..\..\releases\Jdenticon.$version$\net40\Jdenticon.WinForms.dll" target="lib\net40\Jdenticon.WinForms.dll" />
    <file src="..\..\releases\Jdenticon.$version$\netcore30\Jdenticon.WinForms.dll" target="lib\netcoreapp3.0\Jdenticon.WinForms.dll" />
    <file src="..\..\releases\help\Jdenticon.WinForms.xml" target="lib\net40\Jdenticon.WinForms.xml" />
    <file src="..\..\releases\help\Jdenticon.WinForms.xml" target="lib\netcoreapp3.0\Jdenticon.WinForms.xml" />
    <file src="ReadMe.WinForms.txt" target="readme.txt" />
    <file src="nuget-logo-net.png" target="images\jdenticon-logo-net.png" />
  </files>
</package>